      "path": "/admin/user",
      "method": "get",
      "sdk_endpoint": "administration.users",
      "sdk_method": "get",
      "description": "Get all users"
    },
    "createUser_1": {
      "path": "/admin/user", 
      "method": "post",
      "sdk_endpoint": "administration.users",
      "sdk_method": "create",
      "description": "Create a new user"
    },
    "getAaaConfig": {
      "path": "/admin/aaa",
      "method": "get", 
