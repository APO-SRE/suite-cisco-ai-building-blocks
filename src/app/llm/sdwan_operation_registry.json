      "custom_call": "get_aaa_config",
      "description": "Get AAA configuration"
    },
    "getAllDeviceStatus": {
      "path": "/device/status",
      "method": "get",
      "sdk_endpoint": "devices",
      "sdk_method": "get",
      "description": "Get device status"
    },
    "getDevicesHealth": {
      "path": "/device/health/details",
      "method": "get",
